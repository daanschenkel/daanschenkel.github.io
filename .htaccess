# Custom error pages.
ErrorDocument 404 /home.html

# Prevent viewing of htaccess file.
<Files .htaccess>
    order allow,deny
    deny from all
</Files>

Redirect 301 / /home.html
Redirect 301 /index.html home.html

RedirectMatch 301 ^(.*)$ /home.html

# Prevent directory listings
Options All -Indexes
